{
  "name": "@adobe/aio-lib-core-ims",
  "description": "IMS Support Module providing low level IMS HTTP API access as well as higher level support for getting tokens.",
  "version": "1.0.0",
  "author": "Adobe",
  "bugs": "https://github.com/adobe/aio-lib-core-ims/issues",
  "dependencies": {
    "@adobe/aio-lib-core-config": "1.x",
    "debug": "^4.1.1",
    "request-promise-native": "^1.0.5"
  },
  "peerDependencies": {
    "@adobe/aio-lib-core-ims-jwt": "^1",
    "@adobe/aio-lib-core-ims-oauth": "^1"
  },
  "devDependencies": {
    "eslint": "^4",
    "eslint-config-oclif": "^1",
    "jest": "^23.0.1",
    "jest-junit": "^5.0.0",
    "jsdoc-to-markdown": "^5.0.3",
    "stdout-stderr": "^0.1.9"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "files": [
    "/src"
  ],
  "homepage": "https://github.com/adobe/aio-lib-core-ims",
  "keywords": [
    "aio-lib",
    "ims"
  ],
  "license": "Apache-2.0",
  "main": "src/index.js",
  "repository": "https://github.com/adobe/aio-lib-core-ims",
  "scripts": {
    "posttest": "eslint src",
    "test": "npm run unit-tests",
    "unit-tests": "jest --ci",
    "docs": "jsdoc2md --template jsdoc2md/api.hbs src/*.js > api.md",
    "postpack": "",
    "version": "git add README.md"
  },
  "jest": {
    "collectCoverage": true,
    "testPathIgnorePatterns": [
      "<rootDir>/tests/fixtures/"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/tests/fixtures/"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ],
    "testEnvironment": "node",
    "setupTestFrameworkScriptFile": "./jest.setup.js"
  }
}
